{
	"name": "Chromium Monitor",
	"version": "1.1",
	"description": "Monitors active chromium browser tabs",
	"permissions": ["tabs","storage"],
	"background": {
		"scripts": ["monitor.js"],
		"persistent": false
	},
	"browser_action": {
		"default_popup": "browsingdata.html",
		"default_icon": "browsingdata.jpg"
	},
	"manifest_version": 2
}